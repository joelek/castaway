{
	PersonBase: {
		person_id: string,
		name: string
	},

	Person: PersonBase & {

	},

	ArtistBase: {
		artist_id: string,
		title: string
	},

	Artist: ArtistBase & {
		albums: Album[]
	},

	AlbumBase: {
		album_id: string,
		title: string,
		year: number,
		artists: ArtistBase[],
		artwork?: ImageFile
	},

	Album: AlbumBase & {
		discs: Disc[]
	},

	DiscBase: {
		disc_id: string,
		album: AlbumBase,
		number: number
	},

	Disc: DiscBase & {
		tracks: Track[]
	},

	TrackBase: {
		track_id: string,
		title: string,
		disc: DiscBase,
		artists: ArtistBase[],
		number: number,
		last_stream_date?: number
	},

	Track: TrackBase & {
		segment: {
			file: AudioFile
		}
	},

	UserBase: {
		user_id: string,
		name: string,
		username: string
	},

	User: UserBase & {

	},

	PlaylistBase: {
		playlist_id: string,
		title: string,
		description: string,
		user: UserBase
	},

	Playlist: PlaylistBase & {
		items: PlaylistItem[]
	},

	PlaylistItemBase: {
		number: number,
		playlist: PlaylistBase,
		track: Track
	},

	PlaylistItem: PlaylistItemBase & {

	},

	GenreBase: {
		genre_id: string,
		title: string
	},

	Genre: GenreBase & {

	},

	SegmentBase: {
		file: VideoFile,
		subtitles: Subtitle[]
	},

	Segment: SegmentBase & {

	},

	MovieBase: {
		movie_id: string,
		title: string,
		year: number,
		summary: string,
		artwork?: ImageFile,
		last_stream_date?: number,
		genres: Genre[],
		actors: Person[]
	},

	Movie: MovieBase & {
		segment: Segment
	},

	ShowBase: {
		show_id: string,
		title: string,
		summary: string,
		artwork?: ImageFile,
		genres: Genre[],
		actors: Person[]
	},

	Show: ShowBase & {
		seasons: Season[]
	},

	SeasonBase: {
		season_id: string,
		number: number,
		show: ShowBase
	},

	Season: SeasonBase & {
		episodes: Episode[]
	},

	EpisodeBase: {
		episode_id: string,
		title: string,
		summary: string,
		number: number,
		season: SeasonBase,
		year?: number,
		last_stream_date?: number
	},

	Episode: EpisodeBase & {
		segment: Segment
	},

	SubtitleBase: {
		subtitle_id: string,
		file: SubtitleFile,
		language?: string
	},

	Subtitle: SubtitleBase & {
		cues: Cue[]
	},

	CueBase: {
		cue_id: string,
		subtitle: SubtitleBase,
		start_ms: number,
		duration_ms: number,
		lines: string[]
	},

	Cue: CueBase & {

	},

	Entity: Album | Artist | Disc | Episode | Genre | Movie | Season | Show | Track | Playlist | User,

	File: {
		file_id: string,
		mime: string
	},

	AudioFile: File & {
		duration_ms: number
	},

	ImageFile: File & {
		height: number,
		width: number
	},

	SubtitleFile: File & {

	},

	VideoFile: File & {
		duration_ms: number,
		height: number,
		width: number
	}
}
