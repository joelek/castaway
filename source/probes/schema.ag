{
	EpisodeMetadata: {
		type: "episode",
		title: string,
		season: number,
		episode: number,
		year?: number,
		summary?: string,
		show: {
			title: string,
			summary?: string,
			genres: string[],
			actors: string[]
		}
	},

	MovieMetadata: {
		type: "movie",
		title: string,
		year?: number,
		summary?: string,
		genres: string[],
		actors: string[]
	},

	TrackMetadata: {
		type: "track",
		title: string,
		disc: number,
		track: number,
		album: {
			title: string,
			year?: number,
			artists: {
				title: string
			}[]
		},
		artists: {
			title: string
		}[]
	},

	Metadata: EpisodeMetadata | MovieMetadata | TrackMetadata,

	AudioResource: {
		type: "audio",
		duration_ms: number
	},

	ImageResource: {
		type: "image",
		width: number,
		height: number
	},

	MetadataResource: {
		type: "metadata"
	},

	SubtitleResource: {
		type: "subtitle",
		duration_ms: number,
		language?: string
	},

	VideoResource: {
		type: "video",
		duration_ms: number,
		width: number,
		height: number
	},

	Resource: AudioResource | ImageResource | MetadataResource | SubtitleResource | VideoResource,

	Probe: {
		metadata?: Metadata,
		resources: Resource[]
	}
}
