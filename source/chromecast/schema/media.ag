{
	Image: {
		url: string,
		height?: number,
		width?: number
	},

	Volume: {
		level?: number,
		muted?: boolean
	},

	MediaInformation: {
		contentId: string,
		streamType: "NONE" | "BUFFERED" | "LIVE",
		contentType: string,
		metadata?: GenericMediaMetadata | MovieMediaMetadata | TvShowMediaMetadata | MusicTrackMediaMetadata | PhotoMediaMetadata,
		duration?: number,
		customData?: { any }
	},

	GenericMediaMetadata: {
		metadataType: 0,
		title?: string,
		subtitle?: string,
		images?: Image[],
		releaseDate?: string
	},

	MovieMediaMetadata: {
		metadataType: 1,
		title?: string,
		subtitle?: string,
		studio?: string,
		images?: Image[],
		releaseDate?: string
	},

	TvShowMediaMetadata: {
		metadataType: 2,
		seriesTitle?: string,
		subtitle?: string,
		season?: number,
		episode?: number,
		images?: Image[],
		originalAirDate?: string
	},

	MusicTrackMediaMetadata: {
		metadataType: 3,
		albumName?: string,
		title?: string,
		albumArtist?: string,
		artist?: string,
		composer?: string,
		trackNumber?: number,
		discNumber?: number,
		images?: Image[],
		releaseDate?: string
	},

	PhotoMediaMetadata: {
		metadataType: 4,
		title?: string,
		artist?: string,
		location?: string,
		latitude?: number,
		longitude?: number,
		width?: number,
		height?: number,
		creationDateTime?: string
	},

	MediaStatus: {
		mediaSessionId: number,
		media?: MediaInformation,
		playbackRate: number,
		playerState: "IDLE" | "PLAYING" | "BUFFERING" | "PAUSED",
		idleReason?: "CANCELLED" | "INTERRUPTED" | "FINISHED" | "ERROR",
		currentTime: number,
		supportedMediaCommands: number,
		volume: Volume,
		customData?: { any }
	},

	Load: {
		requestId: number,
		type: "LOAD",
		media: MediaInformation,
		autoplay?: boolean,
		currentTime?: number,
		customData?: { any }
	},

	Pause: {
		mediaSessionId: number,
		requestId: number,
		type: "PAUSE",
		customData?: { any }
	},

	Seek: {
		mediaSessionId: number,
		requestId: number,
		type: "SEEK",
		resumeState?: "PLAYBACK_START" | "PLAYBACK_PAUSE",
		currentTime?: number,
		customData?: { any }
	},

	Stop: {
		mediaSessionId: number,
		requestId: number,
		type: "STOP",
		customData?: { any }
	},

	Play: {
		mediaSessionId: number,
		requestId: number,
		type: "PLAY",
		customData?: { any }
	},

	GetStatus: {
		mediaSessionId?: number,
		requestId: number,
		type: "GET_STATUS",
		customData?: { any }
	},

	SetVolume: {
		mediaSessionId: number,
		requestId: number,
		type: "VOLUME",
		volume: Volume,
		customData?: { any }
	},

	InvalidPlayerState: {
		requestId: number,
		type: "INVALID_PLAYER_STATE",
		customData?: { any }
	},

	LoadFailed: {
		requestId: number,
		type: "LOAD_FAILED",
		customData?: { any }
	},

	LoadCancelled: {
		requestId: number,
		type: "LOAD_CANCELLED",
		customData?: { any }
	},

	InvalidRequest: {
		requestId: number,
		type: "INVALID_REQUEST",
		reason: "INVALID_COMMAND" | "DUPLICATE_REQUESTID",
		customData?: { any }
	},

	MediaStatusResponse: {
		requestId: number,
		type: "MEDIA_STATUS",
		status: MediaStatus[],
		customData?: { any }
	}
}
